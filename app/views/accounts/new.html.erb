<div class="panel panel-default">
  <div class="panel-heading">
    <h1> New Account </h1>
  </div>
 <%=render 'form' %>
</div>

<%= form_for :article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
